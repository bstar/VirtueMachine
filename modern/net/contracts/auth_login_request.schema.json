{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://virtuemachine/contracts/auth_login_request.schema.json",
  "title": "AuthLoginRequest",
  "type": "object",
  "required": ["username", "password"],
  "properties": {
    "username": {
      "type": "string",
      "minLength": 2,
      "maxLength": 64
    },
    "password": {
      "type": "string",
      "minLength": 1,
      "maxLength": 256
    }
  },
  "additionalProperties": false
}
